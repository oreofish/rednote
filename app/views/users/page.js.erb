(function() {
  $notes = $('#main-contents ul.list');
  $notes.append( "<%= j render 'notes/list' %> " );

  <% if @notes.size > 0 %>
      setTimeout( function() {
        <% @notes.each do |note| %>
          commentsManager.bindCommentHandler(<%= note.id %>);
          $('#attach_collapse<%= note.id %>').collapse();
        <% end %>
      }, 100 );
    <% end %>
});


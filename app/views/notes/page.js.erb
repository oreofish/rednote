(function() {
  var $notes = $('#main ul.list');
  $notes.append( "<%= j render 'notes/list' %> " );

  <% if @notes.size > 0 %>
      setTimeout( function() {
        <% @notes.each do |note| %>
          commentsManager.bindCommentHandler(<%= note.id %>);
        <% end %>
      }, 100 );
    <% end %>
});


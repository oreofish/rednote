<%# encoding: utf-8 %>

<section id="fluidGridSystem">
  <h3>人员</h3>
  <% User.all.each do |user| %>
    <%= user.nickname %>
  <% end %>

  <h3>任务</h3>
  <% @tasks.each do |task| %>
    <%= render :partial => 'note', :locals => { :task => task } %>
  <% end %>

  <h3>历史任务</h3>
  <% @old_tasks.each do |task| %>
    <%= render :partial => 'note', :locals => { :task => task } %>
  <% end %>

</section>
